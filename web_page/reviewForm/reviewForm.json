    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.password && $data.login && $data.product ){",
        "\treturn {",
        "\t\t//map password variable to expected task contract input",
        "\t\tpasswordInput: {",
        "\t\t\tLogin: $data.password.Login !== undefined ? $data.password.Login : null,",
        "\t\t\tPassword: $data.password.Password !== undefined ? $data.password.Password : null,",
        "\t\t\tAddress: $data.password.Address !== undefined ? $data.password.Address : null,",
        "\t\t\tFirstname: $data.password.Firstname !== undefined ? $data.password.Firstname : null,",
        "\t\t\tLastname: $data.password.Lastname !== undefined ? $data.password.Lastname : null,",
        "\t\t\tEmail: $data.password.Email !== undefined ? $data.password.Email : null",
        "\t\t},",
        "\t\t//map login variable to expected task contract input",
        "\t\tloginInput: {",
        "\t\t\tLogin: $data.login.Login !== undefined ? $data.login.Login : null,",
        "\t\t\tPassword: $data.login.Password !== undefined ? $data.login.Password : null,",
        "\t\t\tAddress: $data.login.Address !== undefined ? $data.login.Address : null,",
        "\t\t\tFirstname: $data.login.Firstname !== undefined ? $data.login.Firstname : null,",
        "\t\t\tLastname: $data.login.Lastname !== undefined ? $data.login.Lastname : null,",
        "\t\t\tEmail: $data.login.Email !== undefined ? $data.login.Email : null",
        "\t\t},",
